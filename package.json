{
  "name": "zipurl",
  "version": "0.2.13",
  "description": "Generate gzip web-safe strings on the browser",
  "author": "Jason Lee <jason@zerodevx.com>",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "build": "rollup -c",
    "size": "gzip-size zipurl.min.js --include-original",
    "prepublishOnly": "run-s format lint build size",
    "test": "npm run build && http-server . -c-1 -o test/"
  },
  "dependencies": {
    "js-base64": "^3.7.3",
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-terser": "0.2.0",
    "ansi-regex": "6.0.1",
    "chai": "4.3.7",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "gzip-size-cli": "5.1.0",
    "http-server": "14.1.1",
    "mocha": "10.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.1",
    "rollup": "3.7.5"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./zipurl.min.js"
    },
    "./package.json": "./package.json",
    "./zipurl.min.js": "./zipurl.min.js"
  },
  "bin": {
    "zipurl": "./bin/zipurl.js",
    "unzipurl": "./bin/unzipurl.js"
  },
  "files": [
    "bin/",
    "index.js",
    "zipurl.min.js"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "license": "ISC",
  "repository": "github:zerodevx/zipurl",
  "keywords": [
    "gzip",
    "base64",
    "url-safe",
    "web-safe-strings",
    "json-compress"
  ]
}
