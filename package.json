{
  "name": "zipurl",
  "version": "0.2.9",
  "description": "Generate gzip web-safe strings on the browser",
  "author": "Jason Lee <jason@zerodevx.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./zipurl.min.js"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "zipurl": "./src/zipurl.js",
    "unzipurl": "./src/unzipurl.js"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "build": "rollup -c",
    "size": "gzip-size zipurl.min.js --include-original",
    "prepublishOnly": "run-s format lint build",
    "test": "http-server . -c-1 -o test/"
  },
  "dependencies": {
    "js-base64": "^3.7.2",
    "pako": "^2.0.4"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.2.1",
    "ansi-regex": "^6.0.1",
    "chai": "^4.3.6",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "gzip-size-cli": "^5.1.0",
    "http-server": "^14.1.0",
    "mocha": "^9.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rollup": "^2.71.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "files": [
    "src/",
    "zipurl.min.js"
  ],
  "license": "ISC",
  "repository": "github:zerodevx/zipurl",
  "keywords": [
    "gzip",
    "base64",
    "url-safe",
    "web-safe-strings",
    "json-compress"
  ]
}
